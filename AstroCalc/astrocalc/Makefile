####### Build rules


all: $(TARGET)

$(TARGET): $(UICDECLS) $(OBJECTS) $(OBJMOC) 
	$(LINK) $(LFLAGS) -o $(TARGET) $(OBJECTS) $(OBJMOC) $(LIBS)

moc: $(SRCMOC)

tmake: Makefile

Makefile: astrocalc.pro
	tmake astrocalc.pro -o Makefile

dist:
	$(TAR) astrocalc.tar astrocalc.pro $(SOURCES) $(HEADERS) $(INTERFACES) $(DIST)
	$(GZIP) astrocalc.tar

clean:
	-rm -f $(OBJECTS) $(OBJMOC) $(SRCMOC) $(UICIMPLS) $(UICDECLS) $(TARGET)
	-rm -f *~ core

